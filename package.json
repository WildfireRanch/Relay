{
  "name": "relay-command-center",
  "version": "1.0.0",
  "description": "FastAPI control plane for Wildfire Ranch agent workflows with Next.js dashboard",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "start": "npm run start --workspace=frontend",
    "lint": "npm run lint --workspace=frontend",
    "typecheck": "npm run typecheck --workspace=frontend",
    "frontend:dev": "npm run dev --workspace=frontend",
    "frontend:build": "npm run build --workspace=frontend",
    "frontend:start": "npm run start --workspace=frontend",
    "backend:dev": "uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "backend:start": "uvicorn main:app --host 0.0.0.0 --port 8000",
    "deploy:railway": "railway up",
    "deploy:vercel": "cd frontend && vercel --prod"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WildfireRanch/relay-command-center"
  },
  "keywords": [
    "fastapi",
    "nextjs",
    "railway",
    "vercel",
    "command-center",
    "agent-workflows"
  ],
  "author": "Wildfire Ranch",
  "license": "UNLICENSED"
}
