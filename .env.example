# File: .env
# Directory: project root
# Purpose: Environment variable configuration for Relay project

# === 🚪 Relay Agent Authentication ===
API_KEY=relay-dev

# === 🤖 OpenAI Configuration ===
OPENAI_API_KEY=s
# === 📄 Google OAuth 2.0 Credentials ===
GOOGLE_CREDS_JSON=
GOOGLE_CLIENT_SECRET=
# === 🔑 OAuth Redirect Configuration ===
OAUTH_REDIRECT_URI=
# === 💻 Execution Environment Mode ===
ENV=local

# === 🛠 Service Endpoints ===
CONTROL_API=

# === 🚀 Deployment ===
NEXT_PUBLIC_RELAY_KEY=

# === 🌐 Frontend API Root (for Next.js frontend, must be NEXT_PUBLIC_*) ===
NEXT_PUBLIC_API_URL=http://localhost:8000
